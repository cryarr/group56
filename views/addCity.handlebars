<h1 class="mt-5">Add region</h1>
<h5 class="mt-5">Add your region</h3>


<form action="/addCity" method="post">
	ID: <input type="text" name="id"><br>
	Name: <input type="text" name="name"><br>
	People ID: <input type="text" name="pid"><br>
	Region ID: <input type="text" name="rid"><br>
	Time periodID: <input type="text" name="time"><br>
	<input type="submit" value="submit"/>
</form>
<br><br><br><br><br>
<h3>List of Events</h3>

<table>
	<thead>
		<tr>
			<th>ID</th>
			<th>City Name</th>
			<th>Population</th>
			<th>rid</th>
			<th>Update/remove</th>
		</tr>
	</thead>
	<tbody>
		{{#each city}}
			<tr>
				<td>{{id}}</td>
				<td>{{city_name}}</td>
				<td>{{population}}</td>
				<td>{{rid}}</td>
				<td><button type="button" id="removeId1">Remove</button>
				<button type="button" id="updateId2">Update</button></td>
			</tr>
		{{/each}}
	</tbody>

</table>
